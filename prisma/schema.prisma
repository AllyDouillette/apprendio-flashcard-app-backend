generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
  email    String @unique
  password String @unique
  id       String @id @default(uuid())
}

model Category {
  id   Int    @id @default(autoincrement())
  name String
}

model Card {
  id Int @id @default(autoincrement())
}
